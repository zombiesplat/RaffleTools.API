{
  "name": "zombiesplat/raffletoolsapi",
  "description": "Tools for non profits to host successful raffles",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Alan Asher",
      "email": "alan@asteriskpound.com"
    }
  ],
  "require-dev": {
    "laravel/homestead": "^3.0",
    "phpunit/phpunit": "5.3.2"
  },
  "require": {
    "php": ">=5.6.0",
    "equip/framework": "^1.6.0",
    "josegonzalez/dotenv": "2.0.0",
    "doctrine/orm": "^2.4",
    "doctrine/migrations": "^1.4",
    "equip/auth": "1.1.1",
    "lcobucci/jwt": "3.1.1",
    "firebase/php-jwt": "v3.0.0",
    "equip/data": "2.3.0",
    "ramsey/uuid-doctrine": "1.2.0",
    "aura/input": "1.1.2",
    "aura/filter": "2.2.0",
    "doctrine/doctrine-module": "1.0.1"
  },
  "autoload": {
    "psr-4": {
      "RaffleTools\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RaffleToolsTests\\": "tests/"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "php --run \"touch('.env');\""
    ]
  }
}
